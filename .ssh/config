Host *
	IdentityFile ~/.ssh/id_ed25519
	ControlMaster auto
	ControlPath ~/.ssh/socket-%r@%h:%p
	Compression yes
	AddressFamily inet
	ServerAliveInterval 120
	ForwardX11 no
	ForwardAgent no

Host grid01
	HostName 10.10.0.1
	User user
	#ForwardAgent yes

Host win10
	HostName 127.0.0.1
	User IEUser
	Port 8022
